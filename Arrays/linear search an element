#include<bits/stdc++.h>
using namespace std;

// linear searching a element 

class Solution{
private:
public:
    int linearSearch(vector<int> &arr,int k)
    {
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i]==k)
            {
                return i;
            }
        }
        return -1;
    }
};
int main()
{
    vector<int> arr={2,3,4,1,5};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int k;
    cin>>k;
    Solution solution;
    auto res=solution.linearSearch(arr,k);
    if(res==-1)
    {
        cout<<"element not found"<<endl;
    }
    else
    {
        cout<<"element at index : "<<res<<endl;
    }
}
