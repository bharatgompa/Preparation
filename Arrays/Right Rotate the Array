#include<bits/stdc++.h>
using namespace std;

// Right Rotate The Array in D Places

class Solution{
private:
public:
    void rightRotateArrayByDPlaces(vector<int> &arr,int d)
    {
        int n=arr.size();
        d=d%n;
        reverse(begin(arr),end(arr));
        reverse(begin(arr),begin(arr)+d);
        reverse(begin(arr)+d,end(arr));
    }
};
int main()
{
    vector<int> arr={1,2,3,4,5,6,7};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int d;
    cin>>d;
    Solution solution;
    solution.rightRotateArrayByDPlaces(arr,d);
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
}
