#include<bits/stdc++.h>
using namespace std;

// sort 0's,1's and 2's

class Solution{
private:
public:
    void dnfSort(vector<int> &arr)
    {
        int low=0,mid=0,high=arr.size()-1;
        while(mid<=high)
        {
            if(arr[mid]==0)
            {
                swap(arr[low],arr[mid]);
                low++;
                mid++;
            }
            else if(arr[mid]==1)
            {
                mid++;
            }
            else
            {
                swap(arr[mid],arr[high]);
                high--;
            }
        }
    }
};
int main()
{
    vector<int> arr={1,1,2,2,0,2,1,0,1};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    Solution solution;
    solution.dnfSort(arr);
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
}
