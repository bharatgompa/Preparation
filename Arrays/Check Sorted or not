#include<bits/stdc++.h>
using namespace std;

// check if array is sorted

class Solution{
private:
public:
    bool checkIfArrayIsSorted(vector<int> &arr)
    {
        int n=arr.size();
        for(int i=0;i<n-1;i++)
        {
            if(arr[i]>arr[i+1])
            {
                return false;
            }
        }
        return true;
    }
};
int main()
{
    vector<int> arr={1,1,1,1,2};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int n=arr.size();
    Solution solution;
    auto res=solution.checkIfArrayIsSorted(arr);
    if(res)
    {
        cout<<"array is sorted"<<endl;
    }
    else
    {
        cout<<"array is not sorted"<<endl;
    }
}
