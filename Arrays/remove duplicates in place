#include<bits/stdc++.h>
using namespace std;

// Remove Duplicates From The Sorted Array

class Solution{
private:
public:
    int removeDuplicatesFromSortedArray(vector<int> &arr)
    {
        int n=arr.size(),i=0;
        for(int j=1;j<n;j++)
        {
            if(arr[i]!=arr[j])
            {
                i++;
                arr[i]=arr[j];
            }
        }
        return i+1;
    }
};
int main()
{
    vector<int> arr={1,1,2,2,2,3,3};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int n=arr.size();
    Solution solution;
    auto res=solution.removeDuplicatesFromSortedArray(arr);
    cout<<"unique elements are : "<<res<<endl;
    for(int i=0;i<res;i++)
    {
        cout<<arr[i]<<" ";
    }
}
