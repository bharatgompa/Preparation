#include<bits/stdc++.h>
using namespace std;

class Solution{
private:
public:
    int findElementByBinarySearch(vector<int> &arr,int target)
    {
        int n=arr.size(),low=0,high=n-1;
        while(low<=high)
        {
            int mid=low+(high-low)/2;
            if(arr[mid]==target)
            {
                return mid;
            }
            else if(arr[mid]<target)
            {
                low=mid+1;
            }
            else
            {
                high=mid-1;
            }
        }
        return -1;
    }
};
int main()
{
    vector<int> arr = {3, 4, 6, 7, 9, 12, 16, 17};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int target=6;
    Solution solution;
    auto res=solution.findElementByBinarySearch(arr,target);
    if(res!=-1)
    {
        cout<<"Given element at index : "<<res<<endl;
    }
    else
    {
        cout<<"Given element is not found"<<endl;
    }
}
