#include<bits/stdc++.h>
using namespace std;

//upper bound is the index of the element which is greater than the given element

class Solution{
private:
public:
    int findUpperBound1(vector<int> &arr,int k)
    {
        return upper_bound(begin(arr),end(arr),k)-begin(arr);
    }
    int findUpperBound2(vector<int> &arr,int k)
    {
        int n=arr.size(),low=0,high=n-1,ans=n;
        while(low<=high)
        {
            int mid=low+(high-low)/2;
            if(arr[mid]>k)
            {
                ans=mid;
                high=mid-1;
            }
            else
            {
                low=mid+1;
            }
        }
        return ans;
    }
};
int main()
{
    vector<int> arr={1,2,2,3};
    for(auto it:arr)
    {
        cout<<it<<" ";
    }
    cout<<endl;
    int k=9;
    Solution solution;
    auto res1=solution.findUpperBound1(arr,k);
    auto res2=solution.findUpperBound2(arr,k);
    cout<<res1<<" "<<res2<<endl;
}


