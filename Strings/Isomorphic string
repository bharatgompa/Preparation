#include<bits/stdc++.h>
using namespace std;

class Solution{
private:
public:
    bool isIsomorphic(string s, string t) {
          int m1[256] = {0}, m2[256] = {0};
          int n = s.size(); 
          for (int i = 0; i < n; ++i) 
          {
              if (m1[s[i]] != m2[t[i]]) return false;
              m1[s[i]] = i + 1;
              m2[t[i]] = i + 1;
          }
          return true;
      }
};
int main()
{
    string s1 = "foo";
    string s2 = "bar";
    Solution solution;
    auto res=solution.isIsomorphic(s1,s2);
    if(res==true)
    {
        cout<<"Yes"<<endl;
    }
    else
    {
        cout<<"No"<<endl;
    }
}

